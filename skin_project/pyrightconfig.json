    "venv": "venv_new",
    "pythonVersion": "3.13",
    "include": [
        "."
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        ".git"
    ],
    "reportMissingImports": "warning",
    "reportMissingModuleSource": "warning",
    "pythonPlatform": "Windows",
